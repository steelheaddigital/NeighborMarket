<table class="table table-bordered table-condensed">
  <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Details and Approval</th>
    </tr>
  </thead>
  <tbody>
    <%@users.each do |user|%>
      <tr>
        <td><%= user.first_name%></td>
        <td><%= user.last_name%></td>
        <td><%= user.address%></td>
        <td><%= link_to_function "Show Details and Edit", "LoadUserDetail('#{edit_user_path(user.id)}')"%></td>
      </tr>
    <%end%>
  </tbody>
</table>

