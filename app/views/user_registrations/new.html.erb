<div class="container">
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <div class="row-fluid">
  <% resource.roles.each do |role| %>
    <%= render :partial => "#{role.name.downcase.camelize.underscore}", :locals => { :f => f } %>
  <% end %>
  </div>
<% end %>
</div>


 